{
    "name": "uma/composer-psysh",
    "type": "composer-plugin",
    "description": "No-frills PsySH-Composer plugin",
    "license": "MIT",
    "require": {
        "php": "^5.5||^7.0||^8.0",
        "composer-plugin-api": "^1.0|^2.0",
        "psy/psysh": "~0.1"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "UMA\\Composer\\PsySH\\": "src/"
        }
    },
    "extra": {
        "class": "UMA\\Composer\\PsySH\\Plugin"
    },
    "config": {
        "sort-packages": true
    }
}
